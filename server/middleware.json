{
  "init": {
    "cors": {
      "enabled": false
    }
  },
  "parse": {
    "body-parser#raw": {
      "params": {
        "limit": "200mb"
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
