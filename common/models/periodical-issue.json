{
  "name": "PeriodicalIssue",
  "plural": "issues",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "String",
      "required": true,
      "id": true
    },
    "title": "String",
    "description": "String",
    "publishDate": "Date",
    "numberYear": "String",
    "numberTotal": "Number"
  },
  "relations": {
    "parent": {
      "type": "belongsTo",
      "model": "Periodical"
    },
    "attachments$": {
      "type": "embedsMany",
      "model": "Attachment",
      "property": "attachments"
    }
  },
  "mixins": {
    "DisableDefaultRemoteMethods": {}
  }
}
